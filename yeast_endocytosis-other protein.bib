pmid = {29851558},
title = {{The contributions of the actin machinery to endocytic membrane bending and vesicle formation}},
url = {http://dx.doi.org/10.1101/172072 https://www.molbiolcell.org/doi/10.1091/mbc.E17-11-0688},
volume = {29},
