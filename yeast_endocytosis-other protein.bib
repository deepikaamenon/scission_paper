month = {nov},
pages = {MPMI--10--18--0281--R},
pmid = {30451565},
title = {{{\textless}i{\textgreater}Magnaporthe oryzae{\textless}/i{\textgreater} Abp1, a MoArk1 kinase-interacting actin binding protein, links actin cytoskeleton regulation to growth, endocytosis, and pathogenesis}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/30451565 https://apsjournals.apsnet.org/doi/10.1094/MPMI-10-18-0281-R},
