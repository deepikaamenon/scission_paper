pages = {493--496},
title = {{Endophilin-A2 functions in membrane scission in clathrin-independent endocytosis}},
url = {http://www.nature.com/nature/journal/v517/n7535/full/nature14064.html http://www.nature.com/nature/journal/v517/n7535/pdf/nature14064.pdf},
volume = {517},
