issn = {0960-9822},
journal = {Current biology : CB},
month = {jul},
number = {13},
pages = {1096--1101},
title = {{Mechanical Forces of Fission Yeast Growth}},
url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC2790036/ https://www.ncbi.nlm.nih.gov/pmc/articles/PMC2790036/pdf/nihms119895.pdf},
volume = {19},
year = {2009}
}
