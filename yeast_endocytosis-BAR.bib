journal = {Molecular and Cellular Biology},
keywords = {Folder - other proteins},
language = {en},
mendeley-tags = {Folder - other proteins},
month = {nov},
number = {22},
pages = {7889--7906},
title = {{Saccharomyces cerevisiae Bzz1p Is Implicated with Type I Myosins in Actin Patch Polarization and Is Able To Recruit Actin-Polymerizing Machinery In Vitro}},
url = {http://mcb.asm.org/content/22/22/7889 http://mcb.asm.org/content/22/22/7889.full.pdf http://mcb.asm.org/content/22/22/7889.long http://www.ncbi.nlm.nih.gov/pubmed/12391157},
volume = {22},
