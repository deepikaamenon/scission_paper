@article{Shimada2007,
abstract = {Pombe Cdc15 homology (PCH) proteins play an important role in a variety of actin-based processes, including clathrin-mediated endocytosis (CME). The defining feature of the PCH proteins is an evolutionarily conserved EFC/F-BAR domain for membrane association and tubulation. In the present study, we solved the crystal structures of the EFC domains of human FBP17 and CIP4. The structures revealed a gently curved helical-bundle dimer of approximately 220 A in length, which forms filaments through end-to-end interactions in the crystals. The curved EFC dimer fits a tubular membrane with an approximately 600 A diameter. We subsequently proposed a model in which the curved EFC filament drives tubulation. In fact, striation of tubular membranes was observed by phase-contrast cryo-transmission electron microscopy, and mutations that impaired filament formation also impaired membrane tubulation and cell membrane invagination. Furthermore, FBP17 is recruited to clathrin-coated pits in the late stage of CME, indicating its physiological role.},
author = {Shimada, Atsushi and Niwa, Hideaki and Tsujita, Kazuya and Suetsugu, Shiro and Nitta, Koji and Hanawa-Suetsugu, Kyoko and Akasaka, Ryogo and Nishino, Yuri and Toyama, Mitsutoshi and Chen, Lirong and Liu, Zhi-Jie and Wang, Bi-Cheng and Yamamoto, Masaki and Terada, Takaho and Miyazawa, Atsuo and Tanaka, Akiko and Sugano, Sumio and Shirouzu, Mikako and Nagayama, Kuniaki and Takenawa, Tadaomi and Yokoyama, Shigeyuki},
doi = {10.1016/j.cell.2007.03.040},
issn = {0092-8674},
journal = {Cell},
keywords = {Actin Cytoskeleton,Animals,COS Cells,Carrier Proteins,Cell Membrane Structures,Cercopithecus aethiops,Clathrin,Coated Pits- Cell-Membrane,Cryoelectron Microscopy,Crystallography- X-Ray,Dimerization,Folder - bar proteins,Humans,Mice,Microtubule-Associated Proteins,Models- Molecular,Mutation,NIH 3T3 Cells,Protein Structure- Tertiary,endocytosis},
language = {eng},
mendeley-tags = {Actin Cytoskeleton,Animals,COS Cells,Carrier Proteins,Cell Membrane Structures,Cercopithecus aethiops,Clathrin,Coated Pits- Cell-Membrane,Cryoelectron Microscopy,Crystallography- X-Ray,Dimerization,Folder - bar proteins,Humans,Mice,Microtubule-Associated Proteins,Models- Molecular,Mutation,NIH 3T3 Cells,Protein Structure- Tertiary,endocytosis},
month = {may},
number = {4},
pages = {761--772},
title = {{Curved EFC/F-BAR-domain dimers are joined end to end into a filament for membrane invagination in endocytosis}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/17512409},
volume = {129},
year = {2007}
}
@article{Myers2016a,
abstract = {Summary
Membrane remodeling by BAR (Bin, Amphiphysin, RVS) domain-containing proteins, such as endophilins and amphiphysins, is integral to the process of endocytosis. However, little is known about the regulation of endocytic BAR domain activity. We have identified an interaction between the yeast Rvs167 N-BAR domain and calmodulin. Calmodulin-binding mutants of Rvs167 exhibited defects in endocytic vesicle release. In vitro, calmodulin enhanced membrane tubulation and constriction by wild-type Rvs167 but not calmodulin-binding-defective mutants. A subset of mammalian N-BAR domains bound calmodulin, and co-expression of calmodulin with endophilin A2 potentiated tubulation in vivo. These studies reveal a conserved role for calmodulin in regulating the intrinsic membrane-sculpting activity of endocytic N-BAR domains.},
author = {Myers, Margaret D. and Ryazantsev, Sergey and Hicke, Linda and Payne, Gregory S.},
doi = {10.1016/j.devcel.2016.03.012},
file = {:Users/deepikaa/Library/Application Support/Mendeley Desktop/Downloaded/N7EIA9R9/Myers et al. - 2016 - Calmodulin Promotes N-BAR Domain-Mediated Membrane.pdf:pdf;:Users/deepikaa/Library/Application Support/Mendeley Desktop/Downloaded/JDBF4MJJ/S1534580716301575.html:html},
issn = {1534-5807},
journal = {Developmental Cell},
keywords = {Folder - bar proteins},
mendeley-tags = {Folder - bar proteins},
month = {apr},
number = {2},
pages = {162--173},
title = {{Calmodulin Promotes N-BAR Domain-Mediated Membrane Constriction and Endocytosis}},
url = {http://www.sciencedirect.com/science/article/pii/S1534580716301575 http://www.sciencedirect.com/science/article/pii/S1534580716301575/pdfft?md5=acdff0a072529e947f82fe54c1dd8ac9{\&}pid=1-s2.0-S1534580716301575-main.pdf},
