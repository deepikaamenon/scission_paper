month = {may},
number = {5},
pages = {848--859},
title = {{Multiple Pathways Regulate Endocytic Coat Disassembly in Saccharomyces cerevisiae for Optimal Downstream Trafficking}},
url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1600-0854.2008.00726.x/abstract http://onlinelibrary.wiley.com/doi/10.1111/j.1600-0854.2008.00726.x/pdf},
volume = {9},
