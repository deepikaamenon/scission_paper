@article{Ayscough2000,
abstract = {Studies using drugs that cause the disassembly of filamentous actin (F-actin) have demonstrated the importance of an intact actin cytoskeleton for polarised secretion by yeast cells [1] and [2]. To address the level of dynamic turnover needed for such processes, however, drugs or mutants that confer stabilising properties on F-actin are needed. Jasplakinolide is the only readily available drug that stabilises F-actin structures both in vivo and in vitro [3], [4], [5] and [6]. Yeast strains have been generated in which two of the ABC multidrug resistance transporter genes have been deleted, rendering normally jasplakinolide-resistant yeast cells sensitive to its effects. Treatment of these cells with jasplakinolide caused rapid and dramatic effects on the actin cytoskeleton, resulting in the accumulation of single large actin structures in cells. These structures, however, still contained components that are normally associated with cortical actin patches. A dynamic actin cytoskeleton was found to be critical for the generation of cell polarity and endocytosis.},
author = {Ayscough, Kathryn R.},
doi = {10.1016/S0960-9822(00)00859-9},
file = {:Users/deepikaa/Library/Application Support/Mendeley Desktop/Downloaded/ARCNVF8E/Ayscough - 2000 - Endocytosis and the development of cell polarity i.pdf:pdf;:Users/deepikaa/Library/Application Support/Mendeley Desktop/Downloaded/ZHDA9Z6A/S0960982200008599.html:html},
issn = {0960-9822},
journal = {Current Biology},
keywords = {Folder - other proteins},
mendeley-tags = {Folder - other proteins},
month = {dec},
number = {24},
pages = {1587--1590},
title = {{Endocytosis and the development of cell polarity in yeast require a dynamic F-actin cytoskeleton}},
