month = {nov},
number = {21},
pages = {3841--3856},
title = {{A Pan1/End3/Sla1 complex links Arp2/3-mediated actin assembly to sites of clathrin-mediated endocytosis}},
url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC4626068/},
volume = {26},
year = {2015}
}
