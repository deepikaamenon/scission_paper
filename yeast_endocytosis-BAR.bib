pages = {73--86},
title = {{A PH Domain in ACAP1 Possesses Key Features of the BAR Domain in Promoting Membrane Curvature}},
url = {http://www.cell.com/article/S1534580714005504/abstract http://www.cell.com/article/S1534580714005504/pdf http://www.cell.com/developmental-cell/abstract/S1534-5807(14)00550-4?elsca1=etoc{\&}elsca2=email{\&}elsca3=1534-5807{\_}20141013{\_}31{\_}1{\_}{\&}elsca4=Cell Press http:},
volume = {31},
