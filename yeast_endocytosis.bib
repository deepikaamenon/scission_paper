pages = {5793--5798},
title = {{Spatial dynamics of receptor-mediated endocytic trafficking in budding yeast revealed by using fluorescent alpha-factor derivatives}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/16574772},
volume = {103},
year = {2006}
}
