journal = {Molecular biology of the cell},
month = {nov},
number = {22},
pages = {3501--14},
pmid = {25143395},
publisher = {American Society for Cell Biology},
title = {{Local and global analysis of endocytic patch dynamics in fission yeast using a new "temporal superresolution" realignment method.}},
