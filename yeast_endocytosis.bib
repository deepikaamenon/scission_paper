pages = {250--255},
shorttitle = {The BAR-domain family of proteins},
title = {{The BAR-domain family of proteins: a case of bending and binding?}},
url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC1299016/ http://www.ncbi.nlm.nih.gov/pmc/articles/PMC1299016/pdf/5-7400105.pdf},
