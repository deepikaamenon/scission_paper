title = {{Rvs161p and Rvs167p, the Two Yeast Amphiphysin Homologs, Function Together in Vivo}},
url = {http://www.jbc.org/content/276/8/6016 http://www.jbc.org/content/276/8/6016.full.pdf http://www.jbc.org/content/276/8/6016.long http://www.ncbi.nlm.nih.gov/pubmed/11096097},
volume = {276},
