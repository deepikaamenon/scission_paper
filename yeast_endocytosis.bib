keywords = {Actin-Related Protein 2-3 Complex,Actins,Clathrin,Folder - other proteins,Luminescent Proteins,Microfilament Proteins,Microscopy- Fluorescence,Models- Biological,Protein Binding,Schizosaccharomyces,Schizosaccharomyces pombe Proteins,Time Factors,endocytosis},
language = {eng},
mendeley-tags = {Actin-Related Protein 2-3 Complex,Actins,Clathrin,Folder - other proteins,Luminescent Proteins,Microfilament Proteins,Microscopy- Fluorescence,Models- Biological,Protein Binding,Schizosaccharomyces,Schizosaccharomyces pombe Proteins,Time Factors,endocytosis},
month = {aug},
number = {16},
pages = {2894--2904},
title = {{Quantitative analysis of the mechanism of endocytic actin patch assembly and disassembly in fission yeast}},
