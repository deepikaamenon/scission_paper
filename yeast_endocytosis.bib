pages = {e40931},
title = {{A Neurotoxic Phospholipase A2 Impairs Yeast Amphiphysin Activity and Reduces Endocytosis}},
url = {http://dx.doi.org/10.1371/journal.pone.0040931 http://www.plosone.org/article/fetchObject.action?uri=info{\%}3Adoi{\%}2F10.1371{\%}2Fjournal.pone.0040931{\&}representation=PDF http://www.plosone.org/article/info{\%}3Adoi{\%}2F10.1371{\%}2Fjournal.pone.0040931},
