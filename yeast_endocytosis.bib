pages = {1805--1812},
title = {{Calmodulin localizes to the spindle pole body of Schizosaccharomyces pombe and performs an essential function in chromosome segregation}},
url = {http://jcs.biologists.org/content/110/15/1805 http://jcs.biologists.org/content/110/15/1805.long http://jcs.biologists.org/content/joces/110/15/1805.full.pdf http://www.ncbi.nlm.nih.gov/pubmed/9264467},
volume = {110},
