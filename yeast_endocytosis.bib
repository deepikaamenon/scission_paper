month = {apr},
number = {16},
pages = {16017--16025},
title = {{The Rsp5 Ubiquitin Ligase Binds to and Ubiquitinates Members of the Yeast CIN85-Endophilin Complex, Sla1-Rvs167}},
url = {http://www.jbc.org/content/279/16/16017 http://www.jbc.org/content/279/16/16017.long http://www.ncbi.nlm.nih.gov/pubmed/14761940},
