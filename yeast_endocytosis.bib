title = {{Actin-Latrunculin A Structure and Function DIFFERENTIAL MODULATION OF ACTIN-BINDING PROTEIN FUNCTION BY LATRUNCULIN A}},
url = {http://www.jbc.org/content/275/36/28120 http://www.jbc.org/content/275/36/28120.full.pdf http://www.ncbi.nlm.nih.gov/pubmed/10859320},
