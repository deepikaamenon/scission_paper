keywords = {Base Sequence,Folder - yeast genetics,Genes- Reporter,Genetic Vectors,Genome- Fungal,Kanamycin Resistance,Molecular Sequence Data,Mutagenesis- Insertional,Polymerase Chain Reaction,Saccharomyces cerevisiae,beta-Galactosidase},
language = {eng},
