title = {{The F-BAR domain protein Syp1 negatively regulates WASp-Arp2/3 complex activity during endocytic patch formation}},
url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC2828323/ http://www.ncbi.nlm.nih.gov/pmc/articles/PMC2828323/pdf/nihms-158665.pdf},
volume = {19},
