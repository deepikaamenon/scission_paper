mendeley-tags = {BRCA2,Folder - sjl,Phosphorylation,actin,nuclear transport,phosphoinositide},
month = {aug},
number = {16},
pages = {4223--4236},
title = {{Regulation of PI4,5P2 synthesis by nuclear–cytoplasmic shuttling of the Mss4 lipid kinase}},
url = {http://emboj.embopress.org/content/22/16/4223 http://emboj.embopress.org/content/embojnl/22/16/4223.full.pdf http://www.ncbi.nlm.nih.gov/pubmed/12912920},
