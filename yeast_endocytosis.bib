title = {{Negative Regulation of Yeast WASp by Two SH3 Domain-Containing Proteins}},
url = {http://www.sciencedirect.com/science/article/pii/S096098220300383X http://www.sciencedirect.com/science/article/pii/S096098220300383X/pdfft?md5=f1c5e02ab394a1c961be4c6587ae6f0e{\&}pid=1-s2.0-S096098220300383X-main.pdf},
volume = {13},
