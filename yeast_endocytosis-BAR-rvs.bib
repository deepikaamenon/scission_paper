title = {{Regulators of yeast endocytosis identified by systematic quantitative analysis}},
url = {http://jcb.rupress.org/content/185/6/1097 http://jcb.rupress.org/content/jcb/185/6/1097.full.pdf http://www.ncbi.nlm.nih.gov/pubmed/19506040},
