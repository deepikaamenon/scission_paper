keywords = {Arp2/3 complex,Caveolae,Clathrin,Folder - other proteins,dynamin,macropinocytosis},
mendeley-tags = {Arp2/3 complex,Caveolae,Clathrin,Folder - other proteins,dynamin,macropinocytosis},
number = {1},
