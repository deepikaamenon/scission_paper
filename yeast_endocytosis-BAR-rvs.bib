shorttitle = {Yeast mutant affected for viability upon nutrient },
title = {{Yeast mutant affected for viability upon nutrient starvation: Characterization and cloning of the RVS161 gene}},
url = {http://onlinelibrary.wiley.com/doi/10.1002/yea.320070708/abstract http://onlinelibrary.wiley.com/store/10.1002/yea.320070708/asset/320070708{\_}ftp.pdf?v=1{\&}t=id0g9trv{\&}s=451baf0f89dbb45c2908cf365e8b8c98d8f6417d},
