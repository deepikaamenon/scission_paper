title = {{Rvs161p Interacts with Fus2p to Promote Cell Fusion in Saccharomyces cerevisiae}},
url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC2132759/ http://www.ncbi.nlm.nih.gov/pmc/articles/PMC2132759/pdf/9801066.pdf},
