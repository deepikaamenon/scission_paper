journal = {Genetics},
keywords = {Arp2/3 complex,Clathrin,Folder - review,S. cerevisiae,endocytosis,membrane},
language = {en},
mendeley-tags = {Arp2/3 complex,Clathrin,Folder - review,S. cerevisiae,endocytosis,membrane},
